  /usr/share/doc/enchant/NEWS
  /usr/share/doc/enchant/README
  /usr/share/doc/enchant/TODO
  /usr/share/man/man1/enchant.1.gz

(libenchant1)
  /usr/bin/cygenchant-1.dll
  /usr/lib/enchant/cygenchant_aspell.dll
  /usr/lib/enchant/cygenchant_ispell.dll
  /usr/lib/enchant/cygenchant_myspell.dll
  /usr/lib/enchant/cygenchant_uspell.dll
  /usr/lib/enchant/libenchant_aspell.la
  /usr/lib/enchant/libenchant_ispell.la
  /usr/lib/enchant/libenchant_myspell.la
  /usr/lib/enchant/libenchant_uspell.la
  /usr/share/enchant/enchant.ordering

(libenchant-devel)
  /usr/include/enchant/enchant++.h
  /usr/include/enchant/enchant-provider.h
  /usr/include/enchant/enchant.h
  /usr/lib/libenchant.dll.a
  /usr/lib/libenchant.la
  /usr/lib/pkgconfig/enchant.pc

